                        android:textColor="#ffffff"
                        android:gravity="center_vertical|center_horizontal"
                        android:textSize="11sp"
                        android:id="@+id/textView7" />
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/ripple_effect"
                        android:padding="4dp"
                        android:layout_gravity="center">
                        <ImageView

                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            app:srcCompat="@drawable/rockt"
                            android:scaleType="centerCrop"
                            android:id="@+id/imageView7"/>
                    </LinearLayout>

                    <TextView
                        android:text="35"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_below="@+id/imageView7"
                        android:background="@drawable/roundback"
                        android:textColor="#ffffff"
                        android:gravity="center_vertical|center_horizontal"
                        android:textSize="11sp"
                        android:id="@+id/s7" />
                </LinearLayout>
            </RelativeLayout>



        </LinearLayout>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="3">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:orientation="horizontal">

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_weight="1"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_width="77dp"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:id="@+id/ll2"
                    android:background="@drawable/roundback"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textView2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/imageView"
                        android:layout_centerHorizontal="true"
                        android:background="@drawable/roundback"
                        android:gravity="center_vertical|center_horizontal"
                        android:text="UNICRON"
                        android:textColor="#ffffff"
                        android:textSize="11sp" />
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/ripple_effect"
                        android:padding="4dp"
                        android:layout_gravity="center">
                        <ImageView

                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            app:srcCompat="@drawable/rockt"
                            android:scaleType="centerCrop"
                            android:id="@+id/imageView2"/>
                    </LinearLayout>

                    <TextView
                        android:text="23"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/imageView"
                        android:layout_centerHorizontal="true"
                        android:background="@drawable/roundback"
                        android:textColor="#ffffff"
                        android:id="@+id/s2"
                        android:gravity="center_vertical|center_horizontal"
                        android:textSize="11sp" />
                </LinearLayout>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="2">


                <TextView
                    android:text="ROUND  :  1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#FFFFFF"
                    android:textSize="16sp"
                    android:id="@+id/round"
                    android:layout_centerInParent="true"
                    android:layout_marginBottom="3dp"/>
                <TextView
                    android:text="time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#ec4c4c"
                    android:layout_centerHorizontal="true"
                    android:layout_centerInParent="true"
                    android:gravity="center"
                    android:layout_marginBottom="3dp"
                    android:textSize="16sp"
                    android:id="@+id/timer"
                    android:layout_toRightOf="@id/round"
                    android:layout_marginLeft="30dp" />

            </RelativeLayout>
            <RelativeLayout
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_width="77dp"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:background="@drawable/roundback"
                    android:id="@+id/ll3"
                    android:orientation="vertical">
                    <TextView
                        android:text="MELISSA"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/imageView"
                        android:layout_centerHorizontal="true"
                        android:background="@drawable/roundback"
                        android:textColor="#ffffff"
                        android:gravity="center_vertical|center_horizontal"
                        android:textSize="11sp"
                        android:id="@+id/textView3" />
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/ripple_effect"
                        android:padding="4dp"
                        android:layout_gravity="center">
                        <ImageView

                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            app:srcCompat="@drawable/rockt"
                            android:scaleType="centerCrop"
                            android:id="@+id/imageView3"/>
                    </LinearLayout>

                    <TextView
                        android:text="17"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/imageView"
                        android:layout_centerHorizontal="true"
                        android:background="@drawable/roundback"
                        android:textColor="#ffffff"
                        android:gravity="center_vertical|center_horizontal"
                        android:textSize="11sp"
                        android:id="@+id/s3" />
                </LinearLayout>
            </RelativeLayout>
        </LinearLayout>
    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="0.6dp"
        android:background="#ffffff"
        android:layout_above="@+id/ll0"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/ll0"
            android:layout_weight="2"
            android:layout_centerInParent="true"
            android:layout_gravity="bottom"
            android:padding="3dp"
            android:layout_margin="4dp">

            <RelativeLayout
                android:id="@+id/l0"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <ImageButton
                    android:id="@+id/imageButton0"
                    android:layout_width="68dp"
                    android:layout_height="68dp"
                    android:padding="4dp"
                    android:layout_centerInParent="true"
                    android:onClick="c0"
                    android:background="@drawable/roundbacktransr"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/rockt" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/l2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1">
                <ImageButton
                    android:layout_width="68dp"
                    android:padding="4dp"
                    android:onClick="c2"
                    android:layout_centerInParent="true"
                    android:layout_height="68dp"
                    android:scaleType="centerCrop"
                    android:background="@drawable/roundbacktrans"
                    app:srcCompat="@drawable/papert"
                    android:id="@+id/imageButton2"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/l4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1">

                <ImageButton
                    android:id="@+id/imageButton4"
                    android:layout_width="68dp"
                    android:padding="4dp"
                    android:onClick="c4"
                    android:layout_height="68dp"
                    android:background="@drawable/roundbacktransp"
                    android:layout_centerInParent="true"
                    android:scaleType="centerCrop"
                    android:soundEffectsEnabled="true"
                    app:srcCompat="@drawable/scissorst" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/l3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1">

                <ImageButton
                    android:id="@+id/imageButton3"
                    android:layout_width="68dp"
                    android:layout_height="68dp"
                    android:padding="4dp"
                    android:clickable="true"
                    android:background="@drawable/roundbacktransg"
                    android:layout_centerInParent="true"
                    android:onClick="c3"
                    android:scaleType="centerCrop"
                    android:soundEffectsEnabled="true"
                    app:srcCompat="@drawable/lizardt" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/l1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1">
                <ImageButton
                    android:layout_width="68dp"
                    android:layout_height="68dp"
                    app:srcCompat="@drawable/spockt"
                    android:padding="4dp"
                    android:background="@drawable/roundbacktransb"
                    android:soundEffectsEnabled="true"
                    android:layout_centerInParent="true"
                    android:onClick="c1"
                    android:scaleType="centerCrop"
                    android:id="@+id/imageButton1"/>
            </RelativeLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1">
                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">
                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:layout_gravity="center"
                            android:id="@+id/tt2"
                            android:maxLines="1"
                            android:background="#000000"
                            android:text=" # 34 "
                            android:textColor="#f2575c"
                            android:textSize="30sp" />
                        <Button
                            android:id="@+id/disapp"
                            android:layout_gravity="center"
                            android:text="Ready"
                            android:onClick="ready"
                            android:layout_centerVertical="true"
                            android:layout_centerInParent="true"
                            android:layout_width="68dp"
                            android:layout_height="70dp">
                        </Button>
                    </RelativeLayout>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>







</LinearLayout>
